{
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": "${ConnectionStrings__DefaultConnection}"
  },

  "SuperUser": {
    "Email": "${SuperUser__Email}",
    "PhoneNumber": "${SuperUser__PhoneNumber}",
    "Password": "${SuperUser__Password}"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Authentication": {
    "Google": {
      "ClientId": "${Authentication__Google__ClientId}",
      "ClientSecret": "${Authentication__Google__ClientSecret}"
    },
    "Facebook": {
      "AppId": "${Authentication__Facebook__AppId}",
      "AppSecret": "${Authentication__Facebook__AppSecret}"
    },
    "Microsoft": {
      "ClientId": "${Authentication__Microsoft__ClientId}",
      "ClientSecret": "${Authentication__Microsoft__ClientSecret}"
    }
  },

  "Paymob": {
    "ApiKey": "${Paymob__ApiKey}",
    "SecretKey": "${Paymob__SecretKey}",
    "PublicKey": "${Paymob__PublicKey}",
    "HMAC": "${Paymob__HMAC}",
    "IntegrationId": "${Paymob__IntegrationId}",
    "Iframe1Id": "${Paymob__Iframe1Id}",
    "Iframe2Id": "${Paymob__Iframe2Id}"
  }
}
